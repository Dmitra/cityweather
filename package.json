{
  "name": "cityweather",
  "version": "0.0.3",
  "description": "Visualization of year city weather data",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.org/Dmitra/cityweather.git"
  },
  "keywords": [
    "weather",
    "city comparison",
    "data visualisation",
    "statistics"
  ],
  "author": "Dmitra",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "start": "webpack-dev-server --config webpack.config.babel.js --port 8000",
    "deploy": "rsync -azP dist/ data image dmitra@daviste.com:/home/dmitra/vis/cityweather",
    "build": "webpack"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "d3": "^3.5.1",
    "eventemitter3": "^4.0.0",
    "leaflet": "^1.5.1",
    "leaflet.markercluster": "^1.4.1",
    "lodash": "4.17.15",
    "plusjs": "https://github.com/Dmitra/plusjs.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@graphiy/eslint-config": "^0.1.0",
    "bourbon": "^6.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "2.1.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  }
}
